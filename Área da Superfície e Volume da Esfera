import java.util.*;

public class treino_aula{
	public static void main(String[] args){
		Scanner in = new Scanner(System.in);
		in.useLocale(Locale.ENGLISH);
		double raio, Area_da_Superficie, Volume_da_Esfera;
		
		raio = in.nextDouble();
		
		Area_da_Superficie = 4*3.1416*raio*raio;
	    Volume_da_Esfera = (4*3.1416*raio*raio*raio)/3;
	    
	    System.out.printf("%.2f\n", Area_da_Superficie);
	    System.out.printf("%.2f\n", Volume_da_Esfera);	   
}
}
