import java.util.Scanner;

public class TreinoAula {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		double num, conversao;
		
		num = Double.parseDouble(in.nextLine());
		
		conversao = num * 100;
		
		System.out.printf("Insira um valor em metros:\n");
		System.out.printf("Resultado: %.1f\n",conversao);
	}
}
