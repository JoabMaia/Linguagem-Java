import java.util.Scanner;

public class TreinoAula {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		double quantl, quantf, tameq, numb, eficiencia;
		
		quantl = Double.parseDouble(in.nextLine());
		quantf = Double.parseDouble(in.nextLine());
		tameq = Double.parseDouble(in.nextLine());
		numb = Double.parseDouble(in.nextLine());
		
		eficiencia = ((quantl/quantf)/tameq - (4.2 * numb));

	    System.out.printf("%.1f", eficiencia);
	}
}
