import java.util.Scanner;

public class TreinoAula {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int valor[] = new int[3], maior, i;
		
		for(i=0; i<3 ;i++)
		{
			valor[i] = in.nextInt();
		}
		maior=valor[0];

		for (i=0;i<3;i++){
		if (valor[i] > maior){
		maior = valor[i];
		}
		}
		System.out.printf("%d\n",maior);	
	}
}
