import java.util.Scanner;

public class TreinoAula{
	public static void main(String[] args){
		Scanner in = new Scanner(System.in);
		int idade;
		
		idade = Integer.parseInt(in.nextLine());
		
		if (idade >= 5 && idade <= 7)
	        System.out.printf("Infantil A\n");

	    if (idade >= 8 && idade <= 10)
	        System.out.printf("Infantil B\n");

	    if (idade >= 11 && idade <= 13)
	        System.out.printf("Juvenil A\n");

	    if (idade >= 14 && idade <= 17)
	    	System.out.printf("Juvenil B\n");

	    if (idade >= 18 && idade <= 40)
	    	System.out.printf("Adulto\n");

	    if (idade > 40)
	    	System.out.printf("Master\n");

	    if (idade < 5)
	    	System.out.printf("Idade invalida.\n");	
}
}
