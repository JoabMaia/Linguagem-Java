import java.util.*;

public class TreinoAula{
	public static void main(String[] args){
		Scanner in = new Scanner(System.in);
		in.useLocale(Locale.ENGLISH);
		double raio1, raio2, area1, area2;
		
		  raio1 = Double.parseDouble(in.nextLine());
		  raio2 = Double.parseDouble(in.nextLine());
	    
		  area1 = 3.14*(raio1*raio1);
	    	  area2 = 3.14*(raio2*raio2);
	    
	    if(area1>area2){
	        System.out.printf("Primeiro circulo\n");
	    }
	    else if(area2>area1){
	        System.out.printf("Segundo circulo\n");
	    }
	    else if(area1==area2){
	        System.out.printf("Iguais\n");
	    }
}
}
