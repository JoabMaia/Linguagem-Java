import java.util.*;

public class treino_aula{
	public static void main(String[] args){
		Scanner in = new Scanner(System.in);
		double valor;
		
		valor = Double.parseDouble(in.nextLine());
		
		if(valor > 7){
	        System.out.printf("Basica\n");
	    }
	    else if(valor < 7){
	        System.out.printf("Acida\n");
	    }
	    else if(valor == 7){
	        System.out.printf("Neutra\n");
	    }
}
}
