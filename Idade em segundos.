import java.util.Scanner;

public class TreinoAula {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		int i,s,m,h,d,a,cs;
		
		i = Integer.parseInt(in.nextLine());
		
		s=1;
	    m=s*60;
	    h=m*60;
	    d=h*24;
	    a=d*365;

	    cs = i*a;

	    System.out.printf("%d", cs);
	}
}
