import java.util.Scanner;

public class Main {
	public static void main(String[] args) {

        Scanner in = new Scanner(System.in);

        int [] val = new int[5];
        int cont = 0;

        for(int i=0; i<5; i++){
            val[i] = in.nextInt();
        }

        for(int i=0; i<5; i++){
            if(val[i]%2 == 0){
                cont++;
            }
        }

        System.out.println(cont + " valores pares");
    }
}
