import java.util.Scanner;

public class TreinoAula {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		double nota1, nota2, nota3, media;
		
		nota1 = Double.parseDouble(in.nextLine());
		nota2 = Double.parseDouble(in.nextLine());
		nota3 = Double.parseDouble(in.nextLine());
		
		media = (nota1+nota2+nota3)/3;
		
		if(media >= 7.0) {
		    System.out.printf("aprovado\n");
		}
		    
		else if(media < 3.0) {
			System.out.printf("reprovado\n");
		}
		    
		else if(media >= 3 && media < 7)
			System.out.printf("prova final\n");	
}
}
