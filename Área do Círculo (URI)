import java.util.Scanner;

public class Treino {
	
	public static double Area(double raio) {
		return 3.14159 * (raio * raio);
	}
	
	public static void main(String[] args) {
		
		Scanner in = new Scanner(System.in);
		
		double raio;
		
		raio = in.nextDouble();
		
		System.out.printf("A=%.4f\n",Area(raio));
		
		in.close();
	}
  
}
