import java.util.Scanner;

public class TreinoAula {
	public static void main(String[] args) {
		Scanner in = new Scanner(System.in);
		double f, c;
		
		f = Double.parseDouble(in.nextLine());
		
		c = (5*(f-32))/9;

		System.out.printf("Digite uma temperatura na escala Fahrenheit:\n");
		System.out.printf("Temperatura em Celsius: %.1f",c);
	}
}
